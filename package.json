{
  "name": "gen-app",
  "description": "Scaffold and generate boilerplate for your next web app",
  "version": "1.0.0",
  "main": "index.js",
  "bin": {
    "gen-app": "lib/index.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && tsc -p .",
    "start": "ts-node src/index.ts",
    "test": "yarn build && node lib/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rousan/gen-app.git"
  },
  "keywords": [
    "scaffold",
    "generate",
    "generator",
    "boilerplate",
    "boilerplate-template"
  ],
  "author": "Rousan Ali <hello@rousan.io> (https://rousan.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rousan/gen-app/issues"
  },
  "homepage": "https://github.com/rousan/gen-app#readme",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "commander": "^2.20.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  }
}
